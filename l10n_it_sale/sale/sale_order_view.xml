<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="l10n_it_sale.sale_order_cigcup_inherit" model="ir.ui.view">
            <field name="name">sale.order.cigcup.inherit</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <field name="company_id" position="after">
                    <group colspan="2" col="2" >
                        <separator string="Fatturazione PA" />
                        <field name="cig" />
                        <field name="cup" />
                    </group>
                </field>
            </field>
        </record>
    </data>
</openerp>
