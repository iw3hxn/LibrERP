            .append(this.contents())
            .attr({
                't-foreach': this.attr('t-foreach'),
                't-as': this.attr('t-as')
            })
            .replaceAll(this)
            .after($(document.createElement('td')).append(
                $(document.createElement('button')).attr({
                    'class': 'oe-edit-row-save', 'type': 'button'})
                  .html(' ')))
